const numbers = [5,98,67,4,32,1,25];

function bubblesort(array){
    const length = array.length;
    for(let i=0;i<length;i++){
        for(j=0;j<length;j++){
            if(array[j]>array[j+1]){
                let temp = array[j];
                array[j] = array[j+1];
                array[j+1] = temp;
            }
        }
    }
}
bubblesort(numbers);
console.log(numbers);
